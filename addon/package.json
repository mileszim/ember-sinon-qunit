{
  "name": "ember-sinon-qunit",
  "type": "module",
  "version": "7.0.0",
  "description": "Sinon sandbox test integration for QUnit",
  "keywords": [
    "ember-addon",
    "sinon",
    "qunit"
  ],
  "bugs": {
    "url": "https://github.com/elwayman02/ember-sinon-qunit/issues"
  },
  "repository": "https://github.com/elwayman02/ember-sinon-qunit",
  "license": "MIT",
  "contributors": [
    "Jordan Hawker <hawker.jordan@gmail.com> (https://www.JordanHawker.com)",
    "Steve Calvert <steve.calvert@gmail.com>"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.4"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0",
    "sinon": "^15.0.1"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "volta": {
    "node": "14.21.1",
    "yarn": "1.22.19"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./test-support": "./dist/test-support/index.js",
    "./sinon-sandbox.js": "./dist/test-support/signon-sandbox.js",
    "./addon-main.js": "./addon-main.cjs",
    "./sinon": "sinon/pkg/sinon-esm"
  },
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "prepublishOnly": "rollup --config",
    "start": "rollup --config --watch"
  },
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2,
    "app-js": {
      "./test-support/index.js": "./dist/_app_/test-support/index.js",
      "./test-support/sinon-sandbox.js": "./dist/_app_/test-support/sinon-sandbox.js"
    }
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@embroider/addon-dev": "^3.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "rollup": "^3.14.0"
  }
}
